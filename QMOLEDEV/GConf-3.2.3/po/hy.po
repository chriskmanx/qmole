#: ../gconf/gconfd.c:842
#, c-format
msgid "starting (version %s), pid %u user '%s'"
msgstr "Սկսվում է (տարբերակ %s), pid %u օգտագործող \"%s\""

#: ../gconf/gconfd.c:890
msgid "Failed to get object reference for ConfigServer"
msgstr "Ձախողում ConfigServer-ի համար օբյեկտի վրա հղում ստանալիս"

#: ../gconf/gconfd.c:919
