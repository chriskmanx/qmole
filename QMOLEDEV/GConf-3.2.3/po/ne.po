#, c-format
msgid "Usage: %s <dir>\n"
msgstr "उपयोग: %s <dir>\n"

#: ../backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"उपयोग: %s <dir>\n"
"  यस्तो मार्कअप ब्याकेण्ड फाइल प्रणाली सोपानक्रम गाभ्नुहोस्:\n"
"   dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"

