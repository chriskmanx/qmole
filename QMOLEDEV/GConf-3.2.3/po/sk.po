#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Použitie: %s <dir>\n"

#: ../backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Použitie: %s <dir>\n"
"  Spojí hierarchiu súborového systému modulu značkovania, ako:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  do:\n"
"    dir/%%gconf-tree.xml\n"

