dist-hook:
	( \
	  builddir=`pwd` ; \
	  cd $(srcdir) && \
	  ( \
	    for dir in $(DISTDIRS) ; do \
	      find $$dir -depth -print | egrep -v '(~$$)' | cpio -pdum $$builddir/$(distdir) 2> /dev/null ; \
	    done \
	  ) \
	)

# If source files missing, see if they can be obtained via VPATH
@WITH_PERL_TRUE@perl-sources:
@WITH_PERL_TRUE@	touch perl-sources && \
@WITH_PERL_TRUE@	cd $(PERLMAGICK) && \
@WITH_PERL_TRUE@	( \
@WITH_PERL_TRUE@	  if test -n "$(VPATH)" ; then \
@WITH_PERL_TRUE@	    for file in "$(VPATH)/$(PERLMAGICK)/"* ; do \
@WITH_PERL_TRUE@	      base=`basename $$file` ; \
@WITH_PERL_TRUE@	      if test ! -r $$base ; then \
@WITH_PERL_TRUE@	        @LN_S@ $$file . ; \
@WITH_PERL_TRUE@	      fi \
@WITH_PERL_TRUE@	    done \
@WITH_PERL_TRUE@	  fi ; \
@WITH_PERL_TRUE@	)

@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@$(PERLMAKEFILE): perl-sources $(PERLMAKEMAKER)
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@	cd $(PERLMAGICK) && @PERL@ Makefile.PL
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@all-perl: $(PERLMAKEFILE)
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@	cd $(PERLMAGICK) && $(MAKE) CC='@CC@' $(PERLALLTARGETS)

@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@install-exec-perl: all-perl
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_TRUE@	cd $(PERLMAGICK) && $(MAKE) CC='@CC@' install

@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@$(PERLMAKEFILE): perl-sources $(PERLMAKEMAKER)
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@	cd $(PERLMAGICK) && @PERL@ Makefile.PL MAP_TARGET=$(PERLSTATICNAME)

@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@all-perl: $(PERLMAKEFILE)
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@	cd $(PERLMAGICK) && $(MAKE) CC='@CC@' $(PERLSTATICNAME)

@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@install-exec-perl: all-perl
@WITH_PERL_TRUE@@WITH_PERL_DYNAMIC_FALSE@@WITH_PERL_STATIC_TRUE@	cd $(PERLMAGICK) && $(MAKE) -f Makefile.aperl CC='@CC@' inst_perl

@WITH_PERL_TRUE@check-perl: $(PERLMAKEFILE)
@WITH_PERL_TRUE@	cd $(PERLMAGICK) && $(MAKE) CC='@CC@' test

@WITH_PERL_TRUE@clean-perl: $(PERLMAKEFILE)
@WITH_PERL_TRUE@	cd $(PERLMAGICK) && $(MAKE) CC='@CC@' clean && rm -f  Makefile.old PerlMagick
@WITH_PERL_TRUE@	rm -f perl-sources

@WITH_PERL_TRUE@distclean-perl: clean-perl

# Satisfy makefile requirements if not building PERL
@WITH_PERL_FALSE@all-perl:
