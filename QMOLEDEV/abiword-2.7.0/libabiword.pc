prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
datarootdir=${prefix}/share
pkgdatadir=${datarootdir}/abiword-2.7
libabiword=true

Name: abiword-2.7
