#!/bin/sh
# tests/aclocal-1.11.  Generated from aclocal.in by configure.

# Be Bourne compatible
if test -n "${ZSH_VERSION+set}" && (emulate sh) >/dev/null 2>&1; then
  emulate sh
  NULLCMD=:
  # Zsh 3.x and 4.x performs word splitting on ${1+"$@"}, which
  # is contrary to our usage.  Disable this feature.
  alias -g '${1+"$@"}'='"$@"'
fi

perllibdir="/home/chris/automake-1.11/lib:/home/chris/automake-1.11/lib"
export perllibdir
# Most of the files are in $srcdir/../m4.  However amversion.m4 is
# generated in ../m4, so we include that directory in the search path too.
exec "/home/chris/automake-1.11/aclocal" $ACLOCAL_TESTSUITE_FLAGS \
     -I "/home/chris/automake-1.11/m4" "--acdir=/home/chris/automake-1.11/m4" ${1+"$@"}
