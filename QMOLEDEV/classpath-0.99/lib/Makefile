install-glibjDATA: $(glibj_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(glibjdir)" || $(MKDIR_P) "$(DESTDIR)$(glibjdir)"
	@list='$(glibj_DATA)'; test -n "$(glibjdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(glibjdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(glibjdir)" || exit $$?; \
	done

uninstall-glibjDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(glibj_DATA)'; test -n "$(glibjdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(glibjdir)'; $(am__uninstall_files_from_dir)
tags: TAGS
