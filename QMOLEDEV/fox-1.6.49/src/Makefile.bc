.cpp.obj:
        $(CXX) -c $(INCDIRS) $(CXXFLAGS) $(DEFINES) $<

all: $(FOXLIB)


clean:
        del $(FOXLIB)
        del *.obj


# Make the library
$(FOXLIB): $(OBJECTS) Makefile.bc.resp
        touch $(FOXLIB)
        del $(FOXLIB)
        $(LIB) "$(FOXLIB)" /P256 @Makefile.bc.resp
