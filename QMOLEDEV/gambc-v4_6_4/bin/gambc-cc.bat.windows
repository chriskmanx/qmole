set LIBS=
set GAMBCLIB=gambc
set GAMBCGSCLIB=gambcgsc
set GAMBCGSILIB=gambcgsi
set LIB_PREFIX=lib
set LIB_EXTENSION=.a
set OBJ_EXTENSION=.o
set EXE_EXTENSION=
set BAT_EXTENSION=

if "%1" == "obj" goto obj
if not "%1" == ""obj"" goto not_obj
:obj

if not "%GAMBC_CC_VERBOSE%" == "yes" goto not_obj_verbose
echo.%C_COMPILER% %FLAGS_OBJ% %DEFS_OBJ% -I"%GAMBCDIR_INCLUDE%" -c -o "%BUILD_OBJ_OUTPUT_FILENAME%" %BUILD_OBJ_CC_OPTIONS% %BUILD_OBJ_INPUT_FILENAMES%
:not_obj_verbose

%C_COMPILER% %FLAGS_OBJ% %DEFS_OBJ% -I"%GAMBCDIR_INCLUDE%" -c -o "%BUILD_OBJ_OUTPUT_FILENAME%" %BUILD_OBJ_CC_OPTIONS% %BUILD_OBJ_INPUT_FILENAMES%

