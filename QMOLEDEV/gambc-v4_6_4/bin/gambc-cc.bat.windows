set BUILD_EXE=%%C_COMPILER%% %%FLAGS_EXE%% %%DEFS_EXE%% -I"%%GAMBCDIR_INCLUDE%%"  -o "%%BUILD_EXE_OUTPUT_FILENAME%%" %%BUILD_EXE_CC_OPTIONS%% %%BUILD_EXE_LD_OPTIONS_PRELUDE%% %%BUILD_EXE_INPUT_FILENAMES%% "%%GAMBCDIR_LIB%%/libgambc.a" %%LIBS%% %%BUILD_EXE_LD_OPTIONS%%

