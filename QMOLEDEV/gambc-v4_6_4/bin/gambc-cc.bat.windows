@echo off

rem The following settings are determined by the configure script.

set C_COMPILER=/usr/bin/gcc
set C_PREPROC=/usr/bin/gcc -E

set FLAGS_OBJ=   -no-cpp-precomp -Wno-unused -Wno-write-strings -O2 -fno-math-errno -fschedule-insns2 -fno-trapping-math -fno-strict-aliasing -fwrapv -fomit-frame-pointer -fPIC -fno-common 
set FLAGS_DYN=   -bundle  -no-cpp-precomp -Wno-unused -Wno-write-strings -O2 -fno-math-errno -fschedule-insns2 -fno-trapping-math -fno-strict-aliasing -fwrapv -fomit-frame-pointer -fPIC -fno-common   -flat_namespace -undefined suppress
set FLAGS_LIB=   -dynamiclib -install_name $(libdir)/$(LIBRARY)   -flat_namespace -undefined suppress
set FLAGS_EXE=    -no-cpp-precomp -Wno-unused -Wno-write-strings -O2 -fno-math-errno -fschedule-insns2 -fno-trapping-math -fno-strict-aliasing -fwrapv -fomit-frame-pointer -fPIC -fno-common  

set DEFS_OBJ= 
set DEFS_DYN= -D___DYNAMIC
set DEFS_LIB= 
set DEFS_EXE= 

set BUILD_OBJ=%%C_COMPILER%% %%FLAGS_OBJ%% %%DEFS_OBJ%% -I"%%GAMBCDIR_INCLUDE%%" -c -o "%%BUILD_OBJ_OUTPUT_FILENAME%%" %%BUILD_OBJ_CC_OPTIONS%% %%BUILD_OBJ_INPUT_FILENAMES%%
set BUILD_DYN=%%C_COMPILER%% %%FLAGS_DYN%% %%DEFS_DYN%% -I"%%GAMBCDIR_INCLUDE%%" -o "%%BUILD_DYN_OUTPUT_FILENAME%%" %%BUILD_DYN_CC_OPTIONS%% %%BUILD_DYN_LD_OPTIONS_PRELUDE%% %%BUILD_DYN_INPUT_FILENAMES%% %%BUILD_DYN_LD_OPTIONS%%
