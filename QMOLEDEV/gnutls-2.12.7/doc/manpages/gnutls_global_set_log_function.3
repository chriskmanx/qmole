.SH NAME
gnutls_global_set_log_function \- API function
.SH SYNOPSIS
.B #include <gnutls/gnutls.h>
.sp
.BI "void gnutls_global_set_log_function(gnutls_log_func " log_func ");"
.SH ARGUMENTS
.IP "gnutls_log_func log_func" 12
