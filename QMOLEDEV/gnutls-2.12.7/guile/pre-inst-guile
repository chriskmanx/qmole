# Sets up the execution environment needed to run the test programs
# and produce the documentation.


GUILE_LOAD_PATH="/home/chris/gnutls-2.12.7/guile/modules:$GUILE_LOAD_PATH"
export GUILE_LOAD_PATH

exec /home/chris/gnutls-2.12.7/libtool --mode=execute		                    \
       -dlopen "/home/chris/gnutls-2.12.7/guile/src/libguile-gnutls-v-1.la"        \
       -dlopen "/home/chris/gnutls-2.12.7/guile/src/libguile-gnutls-extra-v-1.la"  \
        "$@"
