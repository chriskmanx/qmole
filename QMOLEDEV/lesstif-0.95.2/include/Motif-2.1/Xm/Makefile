install-xmDATA: $(xm_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(xmdir)" || $(mkdir_p) "$(DESTDIR)$(xmdir)"
	@list='$(xm_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(xmDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(xmdir)/$$f'"; \
	  $(xmDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(xmdir)/$$f"; \
	done

uninstall-xmDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(xm_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(xmdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(xmdir)/$$f"; \
	done
