#: ../libgnomecanvas/gnome-canvas.c:1407 ../libgnomecanvas/gnome-canvas.c:1408
#: ../libgnomecanvas/gnome-canvas-rich-text.c:227
msgid "Y"
msgstr "Y"

#: ../libgnomecanvas/gnome-canvas.c:2045
msgid "Antialiased"
msgstr "Antyaliasing"

#: ../libgnomecanvas/gnome-canvas.c:2046
msgid "The antialiasing mode of the canvas."
msgstr "Tryb wyświetlania z antyaliasingiem."

#: ../libgnomecanvas/gnome-canvas-text.c:206
#: ../libgnomecanvas/gnome-canvas-rich-text.c:211
msgid "Text"
msgstr "Tekst"

#: ../libgnomecanvas/gnome-canvas-text.c:207
msgid "Text to render"
msgstr "Renderowany tekst"

#: ../libgnomecanvas/gnome-canvas-text.c:215
msgid "Markup"
msgstr "Tekst ze znacznikami"

#: ../libgnomecanvas/gnome-canvas-text.c:216
msgid "Marked up text to render"
msgstr "Wyświetlany tekst opisany znacznikami"

#: ../libgnomecanvas/gnome-canvas-text.c:241
msgid "Font"
msgstr "Czcionka"

#: ../libgnomecanvas/gnome-canvas-text.c:242
msgid "Font description as a string"
msgstr "Opis czcionki w postaci napisu"

#: ../libgnomecanvas/gnome-canvas-text.c:250
msgid "Font description"
msgstr "Opis czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:251
msgid "Font description as a PangoFontDescription struct"
msgstr "Opis czcionki w postaci struktury PangoFontDescription"

#: ../libgnomecanvas/gnome-canvas-text.c:259
msgid "Font family"
msgstr "Rodzina czcionek"

#: ../libgnomecanvas/gnome-canvas-text.c:260
msgid "Name of the font family, e.g. Sans, Helvetica, Times, Monospace"
msgstr "Nazwa rodziny czcionek, np. Sans, Helvetica, Times, Monospace"

#: ../libgnomecanvas/gnome-canvas-text.c:276
#: ../libgnomecanvas/gnome-canvas-text.c:277
msgid "Font style"
msgstr "Styl czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:286
#: ../libgnomecanvas/gnome-canvas-text.c:287
msgid "Font variant"
msgstr "Wariant czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:296
#: ../libgnomecanvas/gnome-canvas-text.c:297
msgid "Font weight"
msgstr "Grubość czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:308
#: ../libgnomecanvas/gnome-canvas-text.c:309
msgid "Font stretch"
msgstr "Rozciągnięcie czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:318
msgid "Font size"
msgstr "Rozmiar czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:319
msgid ""
"Font size (as a multiple of PANGO_SCALE, eg. 12*PANGO_SCALE for a 12pt font "
"size)"
msgstr ""
"Rozmiar czcionki (jako wielokrotność PANGO_SCALE, np. 12*PANGO_SCALE dla "
"czcionki o rozmiarze 12pkt.)"

#: ../libgnomecanvas/gnome-canvas-text.c:329
msgid "Font points"
msgstr "Punkty czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:330
msgid "Font size in points (eg. 12 for a 12pt font size)"
msgstr "Rozmiar czcionki w punktach (np. 12 dla czcionki o rozmiarze 12pkt.)"

#: ../libgnomecanvas/gnome-canvas-text.c:340
msgid "Rise"
msgstr "Wysunięcie"

#: ../libgnomecanvas/gnome-canvas-text.c:341
msgid ""
"Offset of text above the baseline (below the baseline if rise is negative)"
msgstr ""
"Wysunięcie tekstu powyżej linii bazowej (wartość ujemna oznacza przesunięcie "
"poniżej linii bazowej)"

#: ../libgnomecanvas/gnome-canvas-text.c:351
msgid "Strikethrough"
msgstr "Przekreślenie"

#: ../libgnomecanvas/gnome-canvas-text.c:352
msgid "Whether to strike through the text"
msgstr "Określa, czy tekst powinien być przekreślony"

#: ../libgnomecanvas/gnome-canvas-text.c:360
msgid "Underline"
msgstr "Podkreślenie"

#: ../libgnomecanvas/gnome-canvas-text.c:361
msgid "Style of underline for this text"
msgstr "Styl podkreślenia tego tekstu"

#: ../libgnomecanvas/gnome-canvas-text.c:370
msgid "Scale"
msgstr "Skala"

#: ../libgnomecanvas/gnome-canvas-text.c:371
msgid "Size of font, relative to default size"
msgstr "Rozmiar czcionki względem domyślnego rozmiaru"

#: ../libgnomecanvas/gnome-canvas-text.c:425
#: ../libgnomecanvas/gnome-canvas-text.c:433
#: ../libgnomecanvas/gnome-canvas-text.c:441
msgid "Color"
msgstr "Kolor"

#: ../libgnomecanvas/gnome-canvas-text.c:426
msgid "Text color, as string"
msgstr "Kolor tekstu w postaci napisu"

#: ../libgnomecanvas/gnome-canvas-text.c:434
msgid "Text color, as a GdkColor"
msgstr "Kolor tekstu jako GdkColor"

#: ../libgnomecanvas/gnome-canvas-text.c:442
msgid "Text color, as an R/G/B/A combined integer"
msgstr "Kolor tekstu jako liczba całkowita zawierająca R/G/B/A"

#: ../libgnomecanvas/gnome-canvas-text.c:455
msgid "Text width"
msgstr "Szerokość tekstu"

#: ../libgnomecanvas/gnome-canvas-text.c:456
msgid "Width of the rendered text"
msgstr "Szerokość wyświetlanego tekstu"

#: ../libgnomecanvas/gnome-canvas-text.c:463
msgid "Text height"
msgstr "Wysokość tekstu"

#: ../libgnomecanvas/gnome-canvas-text.c:464
msgid "Height of the rendered text"
msgstr "Wysokość wyświetlanego tekstu"

#: ../libgnomecanvas/gnome-canvas-text.c:472
msgid "Font family set"
msgstr "Ustawienie rodziny czcionek"

#: ../libgnomecanvas/gnome-canvas-text.c:473
msgid "Whether this tag affects the font family"
msgstr "Określa, czy ten znacznik wpływa na rodzinę czcionek"

#: ../libgnomecanvas/gnome-canvas-text.c:476
msgid "Font style set"
msgstr "Ustawienie stylu czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:477
msgid "Whether this tag affects the font style"
msgstr "Określa, czy ten znacznik wpływa na styl czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:480
msgid "Font variant set"
msgstr "Ustawienie wariantu czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:481
msgid "Whether this tag affects the font variant"
msgstr "Określa, czy ten znacznik wpływa na wariant czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:484
msgid "Font weight set"
msgstr "Ustawienie grubości czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:485
msgid "Whether this tag affects the font weight"
msgstr "Określa, czy ten znacznik wpływa na grubość czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:488
msgid "Font stretch set"
msgstr "Ustawienie rozciągnięcia"

#: ../libgnomecanvas/gnome-canvas-text.c:489
msgid "Whether this tag affects the font stretch"
msgstr "Określa, czy ten znacznik wpływa na rozciągnięcie czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:492
msgid "Font size set"
msgstr "Ustawienie rozmiaru czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:493
msgid "Whether this tag affects the font size"
msgstr "Określa, czy ten znacznik wpływa na rozmiar czcionki"

#: ../libgnomecanvas/gnome-canvas-text.c:496
msgid "Rise set"
msgstr "Ustawienie wysunięcia"

#: ../libgnomecanvas/gnome-canvas-text.c:497
msgid "Whether this tag affects the rise"
msgstr "Określa, czy ten znacznik wpływa na wysunięcie"

#: ../libgnomecanvas/gnome-canvas-text.c:500
msgid "Strikethrough set"
msgstr "Ustawienie przekreślenia"

#: ../libgnomecanvas/gnome-canvas-text.c:501
msgid "Whether this tag affects strikethrough"
msgstr "Określa, czy ten znacznik wpływa na przekreślenie"

#: ../libgnomecanvas/gnome-canvas-text.c:504
msgid "Underline set"
msgstr "Ustawienie podkreślenia"

#: ../libgnomecanvas/gnome-canvas-text.c:505
msgid "Whether this tag affects underlining"
msgstr "Określa, czy ten znacznik wpływa na podkreślenie"

#: ../libgnomecanvas/gnome-canvas-text.c:508
msgid "Scale set"
msgstr "Ustawienie skali"

#: ../libgnomecanvas/gnome-canvas-text.c:509
msgid "Whether this tag affects font scaling"
msgstr "Określa, czy ten znacznik wpływa na skalę tekstu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:212
msgid "Text to display"
msgstr "Renderowany tekst"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:220
msgid "X position"
msgstr "Pozycja X"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:228
msgid "Y position"
msgstr "Pozycja Y"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:235
msgid "Width"
msgstr "Szerokość"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:236
msgid "Width for text box"
msgstr "Szerokość prostokąta ograniczającego tekstu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:243
msgid "Height"
msgstr "Wysokość"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:244
msgid "Height for text box"
msgstr "Wysokość prostokąta ograniczającego tekstu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:251
msgid "Editable"
msgstr "Modyfikowalny"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:252
msgid "Is this rich text item editable?"
msgstr "Określa, czy element wzbogaconego tekstu jest modyfikowalny."

#: ../libgnomecanvas/gnome-canvas-rich-text.c:259
msgid "Visible"
msgstr "Widoczność"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:260
msgid "Is this rich text item visible?"
msgstr "Określa, czy element wzbogaconego tekstu jest widoczny."

#: ../libgnomecanvas/gnome-canvas-rich-text.c:267
msgid "Cursor Visible"
msgstr "Widoczny kursor"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:268
msgid "Is the cursor visible in this rich text item?"
msgstr ""
"Określa, czy wewnątrz elementu wzbogaconego tekstu widoczny jest kursor."

#: ../libgnomecanvas/gnome-canvas-rich-text.c:275
msgid "Cursor Blink"
msgstr "Migotanie kursora"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:276
msgid "Does the cursor blink in this rich text item?"
msgstr ""
"Określa, czy kursor wewnątrz elementu wzbogaconego tekstu powinien migotać."

#: ../libgnomecanvas/gnome-canvas-rich-text.c:283
msgid "Grow Height"
msgstr "Przyrost wysokości"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:284
msgid "Should the text box height grow if the text does not fit?"
msgstr ""
"Określa, czy wysokość prostokąta ograniczającego element wzbogaconego tekstu "
"powinna wzrastać, jeśli tekst się nie mieści."

#: ../libgnomecanvas/gnome-canvas-rich-text.c:291
msgid "Wrap Mode"
msgstr "Tryb zawijania"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:292
msgid "Wrap mode for multiline text"
msgstr "Tryb zawijania napisów wielowierszowych"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:300
msgid "Justification"
msgstr "Justowanie"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:301
msgid "Justification mode"
msgstr "Tryb justowania"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:309
msgid "Direction"
msgstr "Kierunek"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:310
msgid "Text direction"
msgstr "Kierunek tekstu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:318
msgid "Anchor"
msgstr "Kotwica"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:319
msgid "Anchor point for text"
msgstr "Punkt zakotwiczenia tekstu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:327
msgid "Pixels Above Lines"
msgstr "Miejsce nad wierszami"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:328
msgid "Number of pixels to put above lines"
msgstr "Liczba pikseli umieszczanych nad wierszami"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:336
msgid "Pixels Below Lines"
msgstr "Piksele pod wierszami"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:337
msgid "Number of pixels to put below lines"
msgstr "Liczba pikseli umieszczanych pod wierszami"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:345
msgid "Pixels Inside Wrap"
msgstr "Piksele w zawinięciu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:346
msgid "Number of pixels to put inside the wrap"
msgstr "Liczba pikseli umieszczanych w zawinięciu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:354
msgid "Left Margin"
msgstr "Lewy margines"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:355
msgid "Number of pixels in the left margin"
msgstr "Liczba pikseli wewnątrz lewego marginesu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:363
msgid "Right Margin"
msgstr "Prawy margines"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:364
msgid "Number of pixels in the right margin"
msgstr "Liczba pikseli wewnątrz prawego marginesu"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:372
msgid "Indentation"
msgstr "Wcięcie"

#: ../libgnomecanvas/gnome-canvas-rich-text.c:373
msgid "Number of pixels for indentation"
msgstr "Liczba pikseli we wcięciu"

