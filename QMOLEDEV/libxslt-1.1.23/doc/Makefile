rebuild: api eapi all

install-data-local: 
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)
	-/usr/bin/install -c -m 0644 $(srcdir)/*.html $(DESTDIR)$(TARGET_DIR)
	-/usr/bin/install -c -m 0644 $(srcdir)/*.gif $(DESTDIR)$(TARGET_DIR)
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)/html
	-/usr/bin/install -c -m 0644 $(srcdir)/html/*.html $(DESTDIR)$(TARGET_DIR)/html
	-/usr/bin/install -c -m 0644 $(srcdir)/html/*.png $(DESTDIR)$(TARGET_DIR)/html
	-/usr/bin/install -c -m 0644 $(srcdir)/html/index.sgml $(DESTDIR)$(TARGET_DIR)/html
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)/EXSLT
	-/usr/bin/install -c -m 0644 $(srcdir)/EXSLT/*.html $(DESTDIR)$(TARGET_DIR)/EXSLT
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)/tutorial
	-/usr/bin/install -c -m 0644 $(srcdir)/tutorial/* $(DESTDIR)$(TARGET_DIR)/tutorial
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)/tutorial2
	-/usr/bin/install -c -m 0644 $(srcdir)/tutorial2/* $(DESTDIR)$(TARGET_DIR)/tutorial2

dist-hook:
	(cd $(srcdir) ; tar cvf - *.1 *.html site.xsl news.xsl xsa.xsl \
	  *.gif html/*.html html/*.png tutorial*/libxslt*) | \
	  (cd $(distdir); tar xf -)

.PHONY : html EXSLT EXSLT/html
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
