        property = PyString_AsString(key);
        pspec = g_object_class_find_property(G_OBJECT_GET_CLASS(renderer),
                                             property);
        if (!pspec) {
            PyErr_Format(PyExc_TypeError,
                         "property %s is not defined for the renderer",
